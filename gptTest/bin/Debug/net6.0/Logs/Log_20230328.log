using UnityEngine;
using System.Collections;

public class UIManager : MonoBehaviour
{
    // The game objects that can be added/removed in the inspector
    [SerializeField] private GameObject[] uiObjects;

    void Awake()
    {
        // Iterate through the array of game objects and activate/deactivate each one
        for (int i = 0; i < uiObjects.Length; i++)
        {
            uiObjects[i].SetActive(true);
        }
    }

    // Method for adding/removing game objects in the inspector
    public void ToggleObjects(int index)
    {
        uiObjects[index].SetActive(!uiObjects[index].activeInHierarchy);
    }
}
